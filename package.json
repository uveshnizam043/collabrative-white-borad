{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@hocuspocus/provider": "^2.13.7",
    "@tiptap/core": "^2.8.0",
    "@tiptap/extension-blockquote": "^2.8.0",
    "@tiptap/extension-collaboration": "^2.9.1",
    "@tiptap/extension-collaboration-cursor": "^2.9.1",
    "@tiptap/extension-document": "^2.8.0",
    "@tiptap/extension-link": "^2.8.0",
    "@tiptap/extension-paragraph": "^2.8.0",
    "@tiptap/extension-placeholder": "^2.9.1",
    "@tiptap/extension-text": "^2.8.0",
    "@tiptap/extension-text-align": "^2.8.0",
    "@tiptap/extension-underline": "^2.8.0",
    "@tiptap/starter-kit": "^2.8.0",
    "@tiptap/vue-3": "^2.8.0",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.4",
    "fabric": "^5.4.0",
    "pinia": "^2.1.7",
    "sass": "^1.79.4",
    "sharedb": "^5.1.1",
    "socket.io-client": "^4.7.5",
    "vue": "^3.2.45",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.3.3",
    "vue3-google-oauth2": "^1.0.7",
    "vuedraggable": "^4.1.0",
    "y-prosemirror": "^1.2.12",
    "y-protocols": "^1.0.6",
    "y-socket.io": "^1.1.3",
    "y-websocket": "^2.0.4",
    "yjs": "^13.6.20"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.5",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.4",
    "stylelint": "^16.8.2",
    "stylelint-config-recommended": "^14.0.1",
    "typescript": "~5.4.0",
